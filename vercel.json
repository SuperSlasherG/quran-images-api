{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/page/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/show/page/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/download/page/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
