{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/page/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/show/page/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/download/page/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
